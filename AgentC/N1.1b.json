{
  "name": "SBQC - N1.1b Datalake Janitor Orchestrator",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "hours",
              "hoursInterval": 24
            }
          ]
        }
      },
      "id": "ScheduleTrigger",
      "name": "Daily Schedule (2 AM)",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [
        -200,
        0
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://192.168.2.33:3003/api/v1/storage/scan",
        "authentication": "headerAuth",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "path",
              "value": "/mnt/nas/data"
            },
            {
              "name": "recursive",
              "value": "true"
            }
          ]
        },
        "options": {}
      },
      "id": "TriggerNASScan",
      "name": "Trigger NAS Scan (N2.1)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        50,
        0
      ],
      "credentials": {
        "headerAuth": {
          "id": "DataAPI Key",
          "name": "DataAPI Key"
        }
      }
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://192.168.2.33:3003/integrations/events/n8n",
        "authentication": "headerAuth",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "event",
              "value": "orchestrator_start"
            },
            {
              "name": "workflow",
              "value": "N1.1b"
            },
            {
              "name": "details",
              "value": "Daily Datalake Janitor run started"
            }
          ]
        },
        "options": {}
      },
      "id": "LogStart",
      "name": "Log Start to DataAPI",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        300,
        0
      ],
      "credentials": {
        "headerAuth": {
          "id": "DataAPI Key",
          "name": "DataAPI Key"
        }
      }
    }
  ],
  "connections": {
    "ScheduleTrigger": {
      "main": [
        [
          {
            "node": "TriggerNASScan",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "TriggerNASScan": {
      "main": [
        [
          {
            "node": "LogStart",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {
    "executionOrder": "v1"
  },
  "versionId": "n1.1b-v1",
  "id": "n1.1b",
  "meta": {
    "instanceId": "sbqc-n8n"
  }
}
