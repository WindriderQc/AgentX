{
  "name": "SBQC - N1.2 DataAPI Webhook Receiver",
  "nodes": [
    {
      "parameters": {
        "path": "dataapi-events",
        "options": {}
      },
      "id": "WebhookTrigger",
      "name": "Webhook (DataAPI Events)",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [
        -200,
        0
      ]
    },
    {
      "parameters": {
        "dataType": "string",
        "value1": "={{$json.event}}",
        "rules": {
          "rules": [
            {
              "value2": "scan_complete",
              "output": 1
            },
            {
              "value2": "scan_started",
              "output": 2
            },
            {
              "value2": "error",
              "output": 3
            }
          ]
        }
      },
      "id": "EventSwitch",
      "name": "Switch (Event Type)",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        50,
        0
      ]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://n8n.specialblend.icu/webhook/sbqc-n2-2-hash-enrichment",
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={\n  \"scan_id\": \"={{$json.data?.scan_id || $json.data?.id}}\",\n  \"event\": \"scan_complete\"\n}",
        "options": {}
      },
      "id": "TriggerN2.2",
      "name": "Trigger N2.2 (Hash Enrichment)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        300,
        -100
      ]
    },
    {
      "parameters": {
        "jsCode": "return [{\n  json: {\n    message: \"Scan started\",\n    scan_id: $json.data?.scan_id,\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
      "id": "LogStarted",
      "name": "Log: Scan Started",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        300,
        50
      ]
    },
    {
      "parameters": {
        "jsCode": "return [{\n  json: {\n    message: \"Error received from DataAPI\",\n    error: $json.data?.message || $json.message,\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
      "id": "LogError",
      "name": "Log: Error",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        300,
        200
      ]
    }
  ],
  "connections": {
    "WebhookTrigger": {
      "main": [
        [
          {
            "node": "EventSwitch",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "EventSwitch": {
      "main": [
        null,
        [
          {
            "node": "TriggerN2.2",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "LogStarted",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "LogError",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": true,
  "settings": {},
  "versionId": "1"
}
